<table class="table table-striped">
        <thead>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th></th>
        </thead>
                <tbody>
    <%@project.project_customers.each do |d| %>
            <tr>
                <td><%= link_to d.customer.id, d.customer.person%></td>
                <td><%=d.customer.person.name%></td>
                <td><%=d.customer.person.email%></td>
                <td><%=delete_btn(d)%></td>
            </tr>
        
    <%end%>
    </tbody>
</table>